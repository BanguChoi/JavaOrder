package com.javaOrder.cart.vo;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;

@Entity
public class CartItemVO {
	@Id
	private String item_id;
	
	private String cart_id;
	private String p_id;
	private int itemNum;
	private int itemPrice;
	private int optShot;
	private int optSize;
	private int optTemp;
	private int optSyrup;
}
