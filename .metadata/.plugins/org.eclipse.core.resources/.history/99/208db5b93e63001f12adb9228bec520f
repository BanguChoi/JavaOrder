import com.javaOrder.common.orders.orderitem.repository.OrderItemRepository;

import lombok.RequiredArgsConstructor;

@Service
@RequiredArgsConstructor
public class OrderItemServiceImpl implements OrderItemService {

	
	private final OrderItemRepository orderItemRepository;
	
	@Override
	public List<OrderItem> orderItemList(OrderItem orderItem) {
		List<OrderItem> orderItemList = orderItemRepository.findByorderNumber(orderItem.getOrderNumber().getOrderNumber());
		
		
		return null;
	}
}
